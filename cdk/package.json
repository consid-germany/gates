{
  "name": "@consid-germany/gates",
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/consid-germany/gates.git"
  },
  "author": {
    "name": "Consid GmbH",
    "email": "hamburg@consid.com",
    "organization": true
  },
  "homepage": "https://github.com/consid-germany/gates",
  "license": "MIT",
  "version": "0.0.0",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "prebuild": "node --require ts-node/register copy-builds.ts && tsc -b src/function && node --require ts-node/register src/function/build.ts",
    "build": "jsii",
    "build:watch": "jsii -w",
    "package": "jsii-pacmak",
    "lint": "eslint . --ext .ts",
    "test": "jest",
    "integ-test": "integ-runner --directory ./integ-test --parallel-regions eu-central-1 -v --update-on-failed",
    "integ-watch": "integ-runner --parallel-regions eu-central-1 -v --watch",
    "integ-synth": "npx cdk@latest synth --app \"npx ts-node test/integ.gates.test.ts\" StackUnderTest",
    "check": "prettier src/**/*.ts --check",
    "format": "prettier src/**/*.ts --write"
  },
  "keywords": [
    ""
  ],
  "bugs": {
    "url": "https://github.com/consid-germany/gates/issues"
  },
  "dependencies": {
  },
  "devDependencies": {
    "aws-cdk-lib": "2.131.0",
    "constructs": "10.3.0",
    "@aws-cdk/integ-runner": "^2.131.0-alpha.0",
    "@aws-cdk/integ-tests-alpha": "^2.131.0-alpha.0",
    "@aws-sdk/client-cloudfront": "^3.529.1",
    "@aws-sdk/client-secrets-manager": "^3.529.1",
    "@types/aws-lambda": "^8.10.133",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.17",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "aws-lambda": "^1.0.7",
    "esbuild": "^0.20.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "jsii": "^5.3.15",
    "jsii-pacmak": "^1.94.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "aws-cdk-lib": "2.131.0",
    "constructs": "10.3.0"
  },
  "jsii": {
    "outdir": "dist",
    "tsc": {
      "rootDir": "src",
      "outDir": "lib"
    },
    "versionFormat": "full",
    "excludeTypescript": [
      "src/function/**/*"
    ],
    "targets": {}
  },
  "files": [
    "lib/**/*",
    "build/**/*"
  ],
  "jest": {
    "verbose": true
  },
  "stability": "experimental"
}
